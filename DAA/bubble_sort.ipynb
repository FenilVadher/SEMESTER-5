    "def bubblesort(arr):\n",
    "    for intended in range(len(arr)-1, 0, -1):\n",
    "        for i in range(intended):\n",
    "            if arr[i] > arr[i+1]:\n",
    "                arr[i], arr[i+1] = arr[i+1], arr[i]\n",
    "\n",
    "arr = [3, 2, 5, 1, 4]\n",
    "bubblesort(arr)\n",
    "print(arr)\n"
   ]
